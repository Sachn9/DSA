class Solution {
    public int tupleSameProduct(int[] arr) {
        HashMap<Integer,Integer> map=new HashMap<>();
        for(int i=0;i<arr.length;i++){
            for(int j=i+1;j<arr.length;j++){
                int mul=arr[i]*arr[j];
                map.put(mul,map.getOrDefault(mul,0)+1);
            }
        }

        int res=0;
        for(int freq:map.values()){
            if(freq>=2){
                res += (freq*(freq-1)/2)*8;
            }
        }
        return res;
    }
}
