import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        
        for (int i = 1; i <= N; i += 4) {
            System.out.print(i + " ");
            if (i + 1 <= N) {
                System.out.print((i + 1) + " ");
            }
        }
    }
}
