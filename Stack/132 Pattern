class Solution {
    public boolean find132pattern(int[] nums) {
        int n=nums.length;
        Stack<Integer> st=new Stack<>();
        int third=Integer.MIN_VALUE;

        for(int i=n-1;i>=0;i--){
            if(nums[i]<third){
                return true;
            }

            while(st.size()!=0 && nums[i]>st.peek()){
                third=st.pop();
            }

            st.push(nums[i]);
        }

        return false;
    }
}
