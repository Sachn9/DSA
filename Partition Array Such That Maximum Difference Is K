class Solution {
    public int partitionArray(int[] nums, int k) {

        Arrays.sort(nums);
        int count=1;

        int startMin=nums[0];

        for(int i=1;i<nums.length;i++){
            if(nums[i]-startMin>k){
                count++;
                startMin=nums[i];
            }
        }
        return count;
    }
}
